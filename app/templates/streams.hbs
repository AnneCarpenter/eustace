<br />

<div class="container">
  {{#each model as |stream|}}
    <div class="row">
      <div class="col-md-4">
        <div class="panel panel-default">
          <div class="panel-heading">
            {{stream.name}}
          </div>
        </div>

        <div class="panel panel-default">
          <div class="panel-heading">
            {{push-patch-onto-stream action="create"}}
          </div>
        </div>

        <ul class="list-group">
          {{#each stream.patches as |patch|}}
            <li class="list-group-item">
              {{patch.name}}
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  {{/each}}
</div>

{{outlet}}
